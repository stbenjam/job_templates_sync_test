#!/usr/bin/env ruby

require 'rainbow'

colors = %i(black red green orange)
methods = %i(to_s bright underline)

colors.each do |color|
  methods.each do |method|
    puts Rainbow("The quick brown fox jumped over the lazy dog").send(color).send(method)
  end
end
